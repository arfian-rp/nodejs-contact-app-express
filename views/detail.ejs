<div class="contact mt-3">
  <div class="row">
    <div class="col-md-8">
      <div class="card" style="width: 18rem">
        <div class="card-body">
          <h2>Detail contact</h2>

          <% if (data === "false") { %> nomor tidak ditemukan <br /><br /><a href="/contact" class="btn btn-primary">Kembali</a><% } else { %>

          <p>nama: <%= data.nama %></p>
          <p>no hp: <%= data.nohp %></p>
          <% if (data.email ==="" ) { %> <% data.email = "tidak diisi" %> <% } %>
          <p>email: <%= data.email %></p>
          <a href="/contact" class="btn btn-primary">Kembali</a><% } %>
        </div>
      </div>
    </div>
  </div>
</div>
